{
    "name": "express-api-starter",
    "version": "1.2.0",
    "description": " A basic starter for an express.js API",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "dev": "nodemon src/index.js",
        "inspect": "nodemon --inspect src/index.js",
        "test": "NODE_ENV=test NODE_TLS_REJECT_UNAUTHORIZED=O jest --reporters default jest-stare --coverage --detectOpenHandles --runInBand --forceExit --testTimeout=60000",
        "heroku-postbuild": "npm run seed && npm run migrate",
        "lint": "npm run lint:js ",
        "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
        "lint:js": "npm run lint:eslint src/",
        "lint:fix": "npm run lint:js -- --fix",
        "migrate": "knex migrate:latest",
        "rollback": "knex migrate:rollback",
        "rollback:test": "knex migrate:rollback --env test",
        "seed": "knex seed:run",
        "test:watch": "jest --verbose --watchAll"
    },
    "keywords": [],
    "author": "Sundaybrian",
    "repository": {
        "type": "git",
        "url": ""
    },
    "dependencies": {
        "@sendgrid/mail": "^7.4.2",
        "bcrypt": "^5.0.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.3",
        "helmet": "^4.4.1",
        "http-errors": "^1.8.0",
        "joi": "^17.2.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.5-next2",
        "morgan": "^1.10.0",
        "nodemailer": "^6.4.14",
        "objection": "^2.2.3",
        "pg": "^8.3.0",
        "rootpath": "^0.1.2",
        "swagger-jsdoc": "^6.0.0-rc.3",
        "swagger-ui-express": "^4.1.4",
        "typedi": "^0.8.0",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/helmet": "^4.0.0",
        "@types/node": "^14.14.22",
        "eslint": "^7.18.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "jest": "^26.5.3",
        "jest-serial-runner": "^1.1.0",
        "jest-stare": "^2.2.0",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.7",
        "sqlite3": "^5.0.0",
        "supertest": "^6.1.1",
        "tslint": "^5.20.1",
        "typescript": "^4.1.3"
    }
}
