{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "industry",
    "description": "The industry",
    "required": ["industry_name"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the industry."
        },
        "industry_name": {
            "$id": "#/properties/industry_name",
            "type": "string",
            "title": "The industry_name of the industry."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the industry was deleted."
        }
    }
}
