{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "subscription_type",
    "description": "The subscription_type",
    "required": ["name", "amount", "subscription_duration", "duration_in_days"],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the subscription_type."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the subscription_type."
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description of the subscription_type."
        },
        "amount": {
            "$id": "#/properties/amount",
            "type": "integer",
            "title": "The amount of the subscription_type."
        },
        "duration_in_days": {
            "$id": "#/properties/duration_in_days",
            "type": "integer",
            "title": "The duration_in_days of the subscription_type."
        },
        "subscription_duration": {
            "$id": "#/properties/subscription_duration",
            "type": "string",
            "title": "The subscription_duration of the subscription_type. e.g Day, Week"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the subscription_type was deleted."
        }
    }
}
