{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": " other skill",
    "description": "The  other skill",
    "required": ["skill_name"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the skill."
        },
        "skill_name": {
            "$id": "#/properties/skill_name",
            "type": "string",
            "title": "The skill_name of the skill."
        },
        "freelancer_id": {
            "$id": "#/properties/freelancer_id",
            "type": "integer",
            "title": "The freelancer_id of the skill."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the skill was deleted."
        }
    }
}
