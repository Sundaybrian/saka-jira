{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "company docs",
  "description": "Company docs table",
  "required": ["document_type","description","document_url"],
  "additionalProperties": true,
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The database ID of the document."
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The description of the document eg kra pin etc."
    },
    "document_type": {
      "$id": "#/properties/document_type",
      "type": "string",
      "title": "The document_type of the document."
  },
      "document_number": {
          "$id": "#/properties/document_number",
          "type": "string",
          "title": "The document_number of the document."
      },
      "document_url": {
        "$id": "#/properties/document_url",
        "type": "string",
        "title": "The document_url of the document."
    },
      "company_id": {
          "$id": "#/properties/company_id",
          "type": "integer",
          "title": "The foreign key company_id."
      },
      "created_at": {
          "$id": "#/properties/created_at",
          "type": "string",
          "title": "The creation date when the ticket was created."
      },
      "updated_at": {
          "$id": "#/properties/updated_at",
          "type": "string",
          "title": "The date when the ticket was updated."
      },
      "deleted_at": {
          "$id": "#/properties/deleted_at",
          "type": "string",
          "title": "The date when the skill was deleted."
      }
  }
}
