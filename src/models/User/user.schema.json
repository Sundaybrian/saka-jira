{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "User",
    "description": "The User",
    "required": [
        "first_name",
        "last_name",
        "phone_number",
        "email",
        "password",
        "role"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the user."
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name of the user."
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name of the user."
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email of the user."
        },
        "phone_number": {
            "$id": "#/properties/phone_number",
            "type": "string",
            "title": "The phone_number of the user."
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password of the user."
        },
        "role": {
            "$id": "#/properties/role",
            "type": "string",
            "title": "The role of the user."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the user was deleted."
        }
    }
}
