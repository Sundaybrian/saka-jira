{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Proposal status catalog",
    "description": "The Proposal status catalog",
    "required": ["proposal_status_name"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the Proposal status aka Bid."
        },
        "proposal_status_name": {
            "$proposal_status_name": "#/properties/proposal_status_name",
            "type": "string",
            "title": "The database ID of proposal status."
        },

        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the Proposal aka Bid was deleted."
        }
    }
}
