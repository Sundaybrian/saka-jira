{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "skill",
    "description": "The skill",
    "required": ["proposal_status_name"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the skill."
        },
        "proposal_status_name": {
            "$id": "#/properties/proposal_status_name",
            "type": "string",
            "title": "The proposal_status_name of the skill."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the skill was deleted."
        }
    }
}
