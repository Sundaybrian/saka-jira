{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "payment_type",
    "description": "The payment_type",
    "required": ["payment_type_name"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the payment_type."
        },
        "payment_type_name": {
            "$id": "#/properties/payment_type_name",
            "type": "string",
            "title": "The payment_type_name of the payment_type."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the payment_type was deleted."
        }
    }
}
